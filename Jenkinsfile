    agent any
    
    stages {
        stage("init") {
            steps {
                echo "init"
            }
        }
        stage("build") {
            steps {
                echo "building the app"
            }
        }
        stage("test") {
            steps {
                echo "testing the app"
            }
        }
        stage("deploy") {
            steps {
                echo "deploying the app"
            }
        }
    }
}
